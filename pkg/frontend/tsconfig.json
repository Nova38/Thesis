{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "module": "es2022",
    "noErrorTruncation": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "src/*": [
        "src/*"
      ],
      "app/*": [
        "*"
      ],
      "components_auth/*": [
        "src/components/auth/*"
      ],
      "components_specimen/*": [
        "src/components/specimen/*"
      ],
      "components_collection/*": [
        "src/components/collection/*"
      ],
      "layouts/*": [
        "src/layouts/*"
      ],
      "pages/*": [
        "src/pages/*"
      ],
      "assets/*": [
        "src/assets/*"
      ],
      "boot/*": [
        "src/boot/*"
      ],
      "stores/*": [
        "src/stores/*"
      ],
      "lib/*": [
        "src/lib/*"
      ],
      "@schema/*": [
        "lib/BiochainSchema/*"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "src/lib/**/*.ts"
  ],
  "files": [
    "auto-imports.d.ts"
  ]
}
