{
  "$id": "biochain.v1.Specimen.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "collection_id": {
      "type": "string"
    },
    "georeference": {
      "$ref": "biochain.v1.Specimen.Georeference.schema.json"
    },
    "grants": {
      "additionalProperties": {
        "$ref": "biochain.v1.Specimen.Grant.schema.json"
      },
      "propertyNames": {
        "type": "string"
      },
      "type": "object"
    },
    "images": {
      "additionalProperties": {
        "$ref": "biochain.v1.Specimen.Image.schema.json"
      },
      "propertyNames": {
        "type": "string"
      },
      "type": "object"
    },
    "last_modified": {
      "$ref": "auth.StateActivity.schema.json"
    },
    "loans": {
      "additionalProperties": {
        "$ref": "biochain.v1.Specimen.Loan.schema.json"
      },
      "propertyNames": {
        "type": "string"
      },
      "type": "object"
    },
    "primary": {
      "$ref": "biochain.v1.Specimen.Primary.schema.json"
    },
    "secondary": {
      "$ref": "biochain.v1.Specimen.Secondary.schema.json"
    },
    "specimen_id": {
      "type": "string"
    },
    "taxon": {
      "$ref": "biochain.v1.Specimen.Taxon.schema.json"
    }
  },
  "title": "Specimen",
  "type": "object"
}
