{
  "name": "es",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf ./dist",
    "generate-barrels": "barrelsby --delete -s filesystem -e ./dist/*",
    "build:cjs": "esbuild --bundle index.ts --outfile=./dist/cjs/index.js --platform=node --target=node18 --format=cjs ",
    "build:esm": "esbuild --bundle index.ts --outfile=./dist/esm/index.mjs --platform=node --target=node18 --format=esm ",
    "build:watch": "esbuild --bundle index.ts --outfile=./dist/index.js --platform=node --target=node18 --format=cjs --watch",
    "build": "pnpm run clean && pnpm run generate-barrels && pnpm run build:cjs && pnpm run build:esm "
},
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "barrelsby": "^2.8.1",
    "esbuild": "0.19.7",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  },
  "exports": {
    ".": {
      "module": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "import": "./dist/proxy/index.mjs"
    }
  },
  "packageManager": "pnpm",
  "files": [
    "dist/**"
  ],
  "dependencies": {
    "@bufbuild/protobuf": "^1.4.2",
    "@bufbuild/protoplugin": "^1.4.2",
    "@hyperledger/fabric-gateway": "^1.4.0"
  }
}
