{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/KeySchema",
    "definitions": {
        "KeySchema": {
            "properties": {
                "item_type": {
                    "type": "string",
                    "description": "The item type of the key"
                },
                "item_kind": {
                    "enum": [
                        "ITEM_KIND_UNSPECIFIED",
                        0,
                        "ITEM_KIND_GLOBAL_ITEM",
                        1,
                        "ITEM_KIND_PRIMARY_ITEM",
                        2,
                        "ITEM_KIND_SUB_ITEM",
                        3,
                        "ITEM_KIND_REFERENCE",
                        4
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Item Kind"
                },
                "keys": {
                    "properties": {
                        "paths": {
                            "items": {
                                "type": "string"
                            },
                            "type": "array",
                            "description": "The set of field mask paths."
                        }
                    },
                    "additionalProperties": true,
                    "type": "object",
                    "description": "The paths that make up the key"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Key Schema"
        }
    }
}