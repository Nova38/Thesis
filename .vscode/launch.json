    {
        "name": "Launch file",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "debugAdapter": "dlv-dap",
        "program": "${file}"
    },
    {
        "name": "NoAuth CC",
        "type": "hlf-go",
        "request": "launch",
        "debugAdapter": "dlv-dap",

        "isCaas": true,
        "program": "${workspaceFolder}/packages/saacs/contracts/noauth/bin/main.go",
        "console": "integratedTerminal",
        "env": {
            // "CORE_CHAINCODE_LOGGING_LEVEL": "DEBUG:kvledger,lockbasedtxmgr,ledgerstorage,stateleveldb,statecouchdb,couchdb=debug"
        }
    },
    {
        "name": "Go Containerized Debug",
        "type": "go",
        "request": "attach",
        "mode": "remote",
        "port": 40000,
        "host": "localhost"
    },

